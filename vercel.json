{
  "version": 2,
  "buildCommand": "cd FERRARI && npm ci && npm run build",
  "outputDirectory": "FERRARI/dist",
  "installCommand": "cd FERRARI && npm ci",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}

